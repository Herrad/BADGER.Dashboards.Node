{
    "name": "Releases (Overview)",
    "components": [
    {
          "type": "SharedDataStore",
          "title": "Release Data",
          "storeId": "ReleaseData",
          "dataSource": "elasticsearch",
          "host": "http://badger.laterooms.com:9200",
          "index": "releases-${YYYY}.${MM}",
          "timeProperties": [
            "query.filtered.filter.bool.must.0.range.@timestamp"
          ],
          "defaultTimeFrame": {
            "timeFrame": 0,
            "units": "daysAgo"
          },
          "queries": {
            "modifiers": {
              "today": { }
            },
            "query": {
              "query": {
                "filtered": {
                  "filter": {
                    "bool": {
                      "must": [
                        {
                          "range" : {
                            "@timestamp" : { }
                          }
                        },
                        {
                          "term" : {
                            "environment.raw" : "live"
                          }
                        }
                      ]
                    }
                  }
                }
              },
              "size": 1000
            }
          },
          "mappings": []
        },
        {
            "title": "Release order Telegraph",
            "type": "ReleaseOrderTelegraph",
            "span":"3",
            "url": "http://dashboard-lb.dashboards.logging.laterooms.work:1234/currentState"
        },
        {
            "title": "Shipping",
            "type": "InProgressReleases",
            "span":"6",
            "storeId": "ReleaseData"
        },
        {
            "title": "Shipped",
            "type": "CompletedReleases",
            "span":"3",
            "storeId": "ReleaseData"
        }
    ]
}
